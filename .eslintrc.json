{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "linebreak-style": "off",
    "curly": ["error", "all"],
    "indent": ["error", 2],
    "no-multi-spaces": ["error"]
    },
    "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id", "__"],
          "allowAfterThis": false,
          "allowAfterSuper": false,
          "enforceInMethodNames": true,
        },
      ],
}
